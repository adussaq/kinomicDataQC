{
  "name": "Kinomics",
  "description": "Analyze Kinomic Data",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "content_security_policy": "script-src 'self' https://*.google.com; object-src 'self'",
  "permissions": ["https://www.googleapis.com/fusiontables/v1/"]
}